############
# Database #
############

# The database connection string.
# should start mysql://
DATABASE_URL=

# Upstash redis HTTP endpoint
# You can find this on the Upstash dashboard
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

##################
# Authentication #
##################

# Secret generated by running `openssl rand -base64 32`
NEXTAUTH_SECRET=

# The public production URL.
# Automatically detected when using Vercel.
NEXTAUTH_URL=http://localhost:3000

# Discord OAuth
# https://discord.com/developers/applications
DISCORD_CLIENT_ID=
DISCORD_CLIENT_SECRET=